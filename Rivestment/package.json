{
  "name": "rivestment",
  "version": "0.1.0",
  "description": "A game of MD5 hash collisions",
  "main": "index.js",
  "scripts": {
    "copyhtml": "copyfiles -f lib/www/*.html dist/www/",
    "copycss": "copyfiles -f lib/www/css/*.css node_modules/bootstrap/dist/css/bootstrap.css dist/www/css",
    "copyjs": "copyfiles -f node_modules/markdown/lib/markdown.js node_modules/jquery/dist/jquery.js node_modules/bootstrap/dist/js/bootstrap.js node_modules/react/dist/react.js node_modules/react/dist/react.js node_modules/react-dom/dist/react-dom.js dist/www/js",
    "build": "babel lib -d dist --presets es2015,stage-2 --plugins transform-react-jsx",
    "start": "node server.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel": "6.5.2",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "bootstrap": "3.3.7",
    "express": "4.14.0",
    "express-ws": "2.0.0",
    "jquery": "3.1.1",
    "js-md5": "0.4.1",
    "mongodb": "2.2.11",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "socket.io": "1.5.0",
    "ws": "1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/jalospinoso/rivestment.git"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "copyfiles": "^1.0.0",
    "jsx": "^0.9.89",
    "markdown": "^0.5.0",
    "nodemon": "^1.11.0"
  }
}
